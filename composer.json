{
    "name": "siebird/oauth2-formstack",
    "description": "Formstack OAuth 2.0 Client Provider for The PHP League OAuth2-Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Austin Siewert",
            "email": "austin@siebird.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "authorization",
        "authentication",
        "formstack",
        "formstack api"
    ],
    "require": {
        "php": ">=5.5.0",
        "league/oauth2-client": "~1.2",
        "siebird/oauth2-formstack": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0",
        "mockery/mockery": "~0.9",
        "squizlabs/php_codesniffer": "~2.0"
    },
    "autoload": {
        "psr-4": {
            "Siebird\\OAuth2\\Client\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Siebird\\OAuth2\\Client\\Test\\": "test/src/"
        }
    }
}
